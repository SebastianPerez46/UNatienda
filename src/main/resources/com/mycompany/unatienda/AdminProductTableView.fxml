<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>



<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.unatienda.AdminProductTableController">
   <children>
      <TableView fx:id="adminProductTable" styleClass="table-view" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="colId" minWidth="50.0" prefWidth="50.0" text="ID" />
          <TableColumn fx:id="colNombre" prefWidth="200.0" text="Nombre" />
            <TableColumn fx:id="colCategoria" prefWidth="120.0" text="Categoría" />
            <TableColumn fx:id="colPrecio" minWidth="80.0" prefWidth="100.0" text="Precio" />
            <TableColumn fx:id="colStock" minWidth="60.0" prefWidth="70.0" text="Stock" />
            <TableColumn fx:id="colEliminar" minWidth="100.0" prefWidth="120.0" text="Acción" />
        </columns>
 
      </TableView>
   </children>
</AnchorPane>

